    "@vtex/store-ui": "^0.265.0",
    "babel-preset-gatsby": "^0.5.5",
    "dotenv": "^8.2.0",
    "gatsby": "^2.31.0",
    "gatsby-plugin-manifest": "^2.4.21",
    "gatsby-plugin-netlify": "^2.3.17",
