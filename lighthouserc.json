{
  "ci": {
    "collect": {
      "url": [
        "/",
        "/vintage-phone/p/"
      ],
      "numberOfRuns": 3,
      "staticDistDir": "./public"
    },
    "assert": {
      "preset": "lighthouse:no-pwa",
      "assertions": {
        "uses-long-cache-ttl": "off",
        "categories:pwa": "off",
        "categories:seo": [
          "warn",
          {
            "minScore": 0.7
          }
        ],
        "categories:accessibility": [
          "warn",
          {
            "minScore": 0.7
          }
        ],
        "categories:best-practices": [
          "warn",
          {
            "minScore": 0.8
          }
        ],
        "categories:performance": [
          "error",
          {
            "minScore": 0.9
          }
        ],
        "unused-javascript": [
          "error",
          {
            "maxLength": 3
          }
        ],
        "largest-contentful-paint": [
          "error",
          {
            "maxNumericValue": 3500
          }
        ],
        "first-contentful-paint": [
          "error",
          {
            "maxNumericValue": 2000
          }
        ],
        "interactive": [
          "error",
          {
            "maxNumericValue": 3000
          }
        ],
        "max-potential-fid": [
          "error",
          {
            "maxNumericValue": 300
          }
        ],
        "cumulative-layout-shift": [
          "error",
          {
            "maxNumericValue": 0.2
          }
        ],
        "total-blocking-time": [
          "error",
          {
            "maxNumericalValue": 100
          }
        ]
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}
